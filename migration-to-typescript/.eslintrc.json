{
    "plugins": [
         "prettier",
         "import", 
         "@typescript-eslint"
        ],
    "extends": [
        "plugin:prettier/recommended",
        "prettier",
        "prettier/@typescript-eslint"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "no-debugger": "off",
        "no-console": 0,
        "class-methods-use-this": "off",
        "no-explicit-any": "on",
        "strict-null-checks": "off"
    }
}
